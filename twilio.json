{
    "eventName": "Twilio/message",
    "url": "https://api.twilio.com/2010-04-01/Accounts/{{ACCT}}/Messages",
    "requestType": "POST",
    "auth": {
        "username": "{{ACCT}}",
        "password": "{{TOKEN}}"
    },
    "form": {
        "From" : "{{TO}}",
        "To" : "{{FROM}}",
        "Body" : "{{BODY}}",
        "MediaUrl" : "{{MEDIA}}"
    },
    "mydevices": true
}
